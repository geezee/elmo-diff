{
  "name": "elmo-diff",
  "version": "1.1.1",
  "description": "A fast diffing library",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha --timeout 15000",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha --timeout 15000 -- test/*.js; cat coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "build": "webpack",
    "benchmark": "node benchmarks/benchmarks.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geezee/elmo-diff.git"
  },
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/geezee/elmo-diff/issues"
  },
  "homepage": "https://github.com/geezee/elmo-diff#readme",
  "devDependencies": {
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.5",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13"
  }
}
